{
  "name": "postvel",
  "version": "3.0.0",
  "description": "Maths, physics and chemistry constants",
  "author": "David Kistauri",
  "main": "index.html",
  "scripts": {
    "start": "parcel src/index.pug --open",
    "lint": "stylelint \"src/*/*.sss\" --fix",
    "build": "parcel build src/index.pug -d build",
    "clean": "rm -rf dist/",
    "deploy": "npm run lint && npm run build && cd build && now --prod"
  },
  "keywords": [
    "maths",
    "physics",
    "chemistry",
    "constants",
    "science",
    "parcel",
    "pug"
  ],
  "devDependencies": {
    "cssnano": "^5.0.6",
    "minimist": ">=0.2.1",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "postcss": "^8.3.5",
    "precss": "^4.0.0",
    "pug": "^3.0.1",
    "sugarss": "^4.0.1"
  },
  "dependencies": {
    "now": "^19.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dtroode/postvel"
  },
  "license": "ISC"
}
