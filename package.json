{
  "name": "postvel",
  "version": "3.0.0",
  "description": "Maths, physics and chemistry constants",
  "author": "David Kistauri",
  "scripts": {
    "start": "parcel src/index.pug --open",
    "lint": "stylelint \"src/*/*.sss\" --fix",
    "build": "parcel build src/index.pug",
    "clean": "rm -rf dist/",
    "deploy": "npm run lint && npm run build && cd build && now --prod"
  },
  "keywords": [
    "maths",
    "physics",
    "chemistry",
    "constants",
    "science",
    "parcel",
    "pug"
  ],
  "devDependencies": {
    "@parcel/transformer-pug": "^2.0.0-rc.0",
    "cssnano": "^5.0.8",
    "minimist": ">=1.2.6",
    "parcel": "^2.0.0-rc.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "postcss": "^8.3.7",
    "precss": "^4.0.0",
    "pug": "^3.0.1",
    "sugarss": "^4.0.1"
  },
  "dependencies": {
    "now": "^19.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dtroode/postvel"
  },
  "license": "ISC"
}
